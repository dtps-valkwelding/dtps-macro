{
	"name": "dcm",
	"scopeName": "source.dcm",
	"patterns": [
		{
			"include": "#expressions"
		}
	],
	"repository": {
		"expressions": {
			"patterns": [
				{
					"include": "#comments"
				},
				{
					"include": "#keywords"
				},
				{
					"include": "#numbers"
				},
				{
					"include": "#operators"
				},
				{
					"include": "#support"
				},
				{
					"include": "#variables"
				},
				{
					"include": "#strings"
				},
				{
					"include": "#constant"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.dcm",
					"match": "(?<!.*\\w+.*)#(.*)"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.dcm",
					"match": "(?<!.*\\w+.*)((LOOP|ELSEIF|IF|SELECT|CASE)(?=.*\\())|(THEN)\\s*$|(?<!.*\\w+.*)(ENDLOOP|ELSE|ENDIF|ENDSELECT)\\b"
				},
				{
					"name": "keyword.control.dcm",
					"match": "(OTHERWISE|(?<!.*\\w+.*)(STOPALL|STOP|BREAK|CONTINUE))\\b"
				}
			]
		},
		"constant": {
			"patterns": [
				{
					"name": "constant.character.escape.dcm",
					"match": "(~n|~t|~s|~1|~2|~3|~l|~r|~L|~R|~T|~CA|~CD|~PA|~PD|~W|\\$\\$)"
				}
			]
		},
		"numbers": {
			"patterns": [
				{
					"match": "\\b\\d+\\b",
					"name": "constant.numeric.dcm"
				}
			]
		},
		"operators": {
			"patterns": [
				
				{
					"name": "keyword.other.operator.dcm",
					"match": "(\\||&|==|<=|>=|<|>|!=)"
				},
				{
					"comment": "arithmetic operators",
					"name": "support.constant.math.dcm",
					"match": "[\\*\\-\\+\\=]"
				}
			]
		},
		"support": {
			"patterns": [
				{
					"comment": "COMMANDS",
					"name": "entity.name.type.dcm",
					"match": "(?<!.*\\w+.*)\\b(ADDCOMLIST|ADDELEMENT|ADDMECHJOINT|ADDMOVECSR|ADDSEQUENCECSR|ARGUMENT|ATTACHINSTMODEL|BACKUPDATA|BREAKPOINT|BYTESIZETOSTR|CALCEAXIS|CALCINSTVALUE|CALCINTERSECTORCHPOS|CALCONESTEPPOSELIST|CALCMESH|CALCPOSE|CALCROBMEMORY|CALCWELDLINEAXIS|CHANGECOMPATH|CHANGEMECH|CHANGEMODELINDEX|CHANGESAVEPROGRAMPATH|CHECKPROGRAM|CHOOSE|CLEARCOMLIST|CLEARPOSELIST|CLEARPROGRAM|CLEARUPDATEWELDSEQPARAM|CLEARWORK|CLOSE|CLOSEPARTS|CLOSEPROGRAM|COLLISIONCHECK|COLLISIONCHECKLIST|COMEXEC|COMPARESTR|COPYEAXISVAL|COPYELEMENT|COPYFILE|COPYMOVECOMMAND|COPYPOSE|COPYPROGRAMSTEP|COUNTOFDAY|CREATEMESH|CREATEPOSE|CREATETEMPINSTMODEL|DATE|DATETOSTR|DELEELEMENT|DELFILE|DELMECHJOINT|DELPROGRAMSTEP|DELSTR|DTPSCOMMAND|EDITSEQUENCECSR|ERROROPERATION|EXPORTPROGRAM|FILEDIALOG|FILEENCODE|FINDPATH|FINDSTR|FOLDERDIALOG|GETCOMERRMSG|GETCOMLIST|GETCOMPARAMPC|GETCOMPARAMROB|GETCSRARG|GETCSRSTR|GETCSRVAL|GETCURMACROPOS|GETDISPLAYSTATE|GETEAXISVALFROMPOSE|GETELEMENT|GETELEMENTINFO|GETELEMENTVERTEX|GETGLOBALMAT|GETGLOBALPOSE|GETGLOBALVAL|GETGLOBALVEC|GETINSTALLATIONINFO|GETLANGID|GETMACROTYPE|GETMECHINFO|GETMECHNO|GETMONITORINFO|GETOPENPROGRAMINFO|GETOWNERPROCESS|GETPARTSINFO|GETPOSEFROMLIST|GETPOSENAME|(GETPROGRAM(COMMENT|POSE|STEPNUM|STEPTYPE|VARNAME|VARNUM|VARVAL|))|GETREGDATA|GETROBFILELIST|GETSAVEPROGRAMPATH|GETUPDATEWELDSEQPARAM|GETUPDATEWELDSEQPARAMCOUNT|GETWELDER|GETWELDTABLEPARAM|GETWORKEXTRAPARAM|GETWORKFILE|GETWORKPOS|INPUTBOX|INSERTPROGRAM|INSERTSTR|INSTFILEDIALOG|INSTR|JOINSTR|KIN_CALCARC|KIN_COPYMATRIX|KIN_COPYVECTOR|KIN_CROSS|KIN_DOT|KIN_EULER|KIN_EULER_RPY|KIN_INVERSE|KIN_MATOUTR|KIN_MATRIXTOPOS|KIN_MULTIPLY|KIN_NORMALIZEMATRIX|KIN_NORMALIZEVECTOR|KIN_OFFSETMATRIX|KIN_OFFSETVECTOR|KIN_POSTOMATRIX|KIN_PROJECTION|KIN_ROT|KIN_ROTX|KIN_ROTXYZ|KIN_ROTY|KIN_ROTZ|KIN_SETMATRIXTOVECTOR|KIN_SETVECTORTOMATRIX|KIN_TRANS|KIN_TRANSFORMCOORD|KIN_TRANSFORMVECTOR|LEFTSTR|LOADCOLLISIONPARAM|LOADDISPLAYSTATE|LOADPARAM|LOADTEMPINSTMODEL|LOADWELDTABLE|LOADWORK|LOWERSTR|MATTOSTR|MIDSTR|MIRRORELEMENT|MIRRORPOSE|MESSAGEBOX|MODMOVECSR|MODMOVEPARAM|MODMOVESTEP|MODSEQUENCECSR|MOTIONTIME|MOVEELEMENT|MOVEFILE|MOVEMODEL|OPEN|OPENPARTS|OPENPROGRAM|OUTINT|OUTMAT|OUTPT|OUTSTEP|OUTSTR|OUTSTRVAR|OUTTAG|OUTVAL|OUTVEC|PATHEXISTS|PATHSTR|RECOMPUTEPROGRAM|RELOADPARTSMODEL|RENUMBERPROGRAM|REPLACECSR|REPLACEPOSENAME|REPLACESTR|RESETVARIABLE|RETURN|REWINDLINE|RIGHTSTR|ROTATEELEMENT|SAVECONTROLLER|SAVEDISPLAYSTATE|SAVEPARTS|SAVEPROGRAM|SAVESTLFILE|SCALEELEMENT|SEARCHCSR|SEARCHELEMENT|SEARCHSTEP|SEEK|SELECTPOSE|SETCOMPARAMPC|SETCOMPARAMROB|SETDISPLAYSTATE|SETEAXISVAL|SETEAXISVALTOPOSE|SETELEMENTINFO|SETELEMENTVERTEX|SETGLOBALMAT|SETGLOBALPOSE|SETGLOBALVAL|SETGLOBALVEC|SETGUIPARENT|SETPARTSINFO|SETPOSENAME|SETPROGRAMCOMMENT|SETPROGRAMVARNAME|SETPROGRAMVARNUM|SETPROGRAMVARVAL|SETSIMULATIONPARAM|SETSTR|SETUPDATEWELDSEQPARAM|SETVALUE|SETWELDER|SETWORKPOS|SHIFTPOSE|SHIFTPOSEEX|SHOW3DWINDOW|SHOWCOMPARAM|SHOWDEBUGCONSOLE|SHOWLIGHT|SHOWMODELITEM|SHOWPROGRAMLISTWINDOW|SIMULATEPROGRAM|SPLISTSTR|SUBROUTINE|TRACE|TRANSLATEELEMENT|TRIMSTR|UPDATEWELDSEQ|UPPERSTR|VECTOSTRWATCHLIST)\\b"
				},
				{
					"comment": "FUNCTIONS",
					"name": "storage.type.function.dcm",
					"match": "(?<!.*\\w+.*)\\b(FUNCTION|ENDFUNCTION|LOADFUNCTION|UNLOADFUNCTION|OPENDLL|DLLFUNCTION|CLOSEDLL)\\b"
				},
				{
					"comment": "FUNCTIONS import",
					"name": "support.function.dcm",
					"match": "(?<!.*\\w+.*)\\b(CALLFUNCTION)"
				},
				{
					"comment": "ARGUMENTS/PARAMTERS",
					"name": "keyword.other.unit.dcm",
					"match": "(?<=\\(.*)\\b(NEXT|FIND|NONE|OK|STRING|INTEGER|INOUT|INPUT|ITEM|COMBOBOX|COMBOBOXEX|VECTOR|OKCANCEL|OKRETRY|OKRETRYCANCEL|DOUBLE|SHOW|IMAGE|STANDARD|BASIC|GROUP|OBJECT|INDEPENDENT|CURRENT|START|END|INFORMATION|STOP|INDEX|EXTENDMODE|UNDELETE|LINE|VERTEXNUM|SYSTEM|WIDTH|COUNT|DEFAULTNOTOPMOST|TOP|BOTTOM|TOPMOST|ALL|MATRIX|POSE|STDOUT|STRVAR|ONCE|ALWAYS|REDIRECTION|ZORDER|NOW|YEAR|WEEK|SET|MONTH|HOUR|DAY|MINUTE|SECOND|YESTERDAY|TOMORROW|TODAY|SEND|SAVEPATHASS|AES|MNU|SLS|TSF|STR|ADD|NEW|TAG|ALLTAGS|DISABLE|DEFAULT|TOOLSHIFTMODE|ROTATION|VALID|EXECUTE|MODIFY|MODEL|TOOL|WORK|ROBOT|POINT|BASICPOINT|COORDTYPE|STEP|NULL|MOVEL|MOVEP|MOVEC|MOVEL+|MOVEP+|MOVEC+|MOVECW|MOVECW+|MOVELW|MOVELW+|WEAVEP|WEAVEP+|INVALID|MACRO|FILE|FULLPATH|OUTPUT|REMOVE|FILESPEC|GET|PROGRAM|PCTOOLDATA|EXCLAMATION|APPEND|VOID|BACKSLASH|SEQUENCE|PASTE|ADDITEM|CLEAR|MOVE|CALC|HIDE|VAR|NUM|RETRYCANCEL|OVERWRITE|ENABLE|WORKAREA|UPDATE|SELECT|SELRANGE|FORWARD|SHIFTER|STEPBYSTEP|TRANSLATION|EXTAXIS|SOFTLIMIT|SHIFTERPRM|READ|BOTH|REVERSE|PREV|WELDST|WELDED|REFP|HARMONIC|COMMAND|WELD|AIR|STATUS|CIRCLE|WELDFLAG|PRIMARY|LOCATION|RIGHT|HOME|MAT|VEC)\\b(?=.*\\)\\s*$)"
				},
				{
					"name": "storage.modifier.dcm",
					"match": "((strtodbl|strtoint)(?=\\s*\\())"
				},
				{
					"name": "storage.other.dcm",
					"match": "((ismat|ispt|isstr|isval)(?=\\s*\\())"
				},
				{
					"name": "support.constant.math.dcm",
					"match": "\\b((abs|acos|asin|atan|atan2|ceil|cos|dot|exp|floor|log|log10|max|min|mod|pow|rand|sign|sin|sqrt|strlen|tan|vlen|vlensq)(\\s*\\())"
				}
			]
		},
		"variables": {
			"patterns": [
				{
					"name": "storage.type.variable.other.dcm",
					"match": "\"((\\%\\.?\\d+\\$)|\\$)\\w+\""
				},
				{
					"name": "variable.dcm",
					"match": "\\$(\\w)+(\\w+|\\[.*\\]|\\.?)*"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.dcm",
					"match": "\"([^\"]*)\""
				},
				{
					"name": "string.quoted.single.dcm",
					"match": "'([^']*)'"
				}
			]
		}
	},
	"semanticHighlighting": true
}